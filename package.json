{
    "name": "tree-sitter-scl",
    "version": "1.0.0",
    "description": "Tree-sitter grammar for SCL (Smart Contracts Language)",
    "main": "bindings/node",
    "types": "bindings/node",
    "keywords": [
        "tree-sitter",
        "scl",
        "smart-contracts"
    ],
    "author": "Yurii Biurher",
    "license": "Apache-2.0",
    "dependencies": {
        "node-addon-api": "^8.5.0",
        "node-gyp-build": "^4.8.4"
    },
    "devDependencies": {
        "tree-sitter-cli": "^0.25.10",
        "prebuildify": "^6.0.1"
    },
    "peerDependencies": {
        "tree-sitter": "^0.25.0"
    },
    "peerDependenciesMeta": {
        "tree-sitter": {
            "optional": true
        }
    },
    "scripts": {
        "build": "tree-sitter build",
        "build-wasm": "tree-sitter build --wasm",
        "test": "tree-sitter test",
        "install": "node-gyp-build",
        "prebuildify": "prebuildify --napi --strip",
        "playground": "tree-sitter playground",
        "highlight": "cat examples/sample.scl | tree-sitter highlight --query-paths queries/highlights.scm /dev/stdin",
        "generate": "tree-sitter generate",
        "test-watch": "tree-sitter test --debug",
        "parse": "tree-sitter parse examples/sample.scl"
    },
    "tree-sitter": [
        {
            "scope": "source.scl",
            "file-types": [
                "scl"
            ],
            "injection-regex": "^scl$"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ZoralLabs/tree-sitter-scl.git"
    },
    "bugs": {
        "url": "https://github.com/ZoralLabs/tree-sitter-scl/issues"
    },
    "homepage": "https://github.com/ZoralLabs/tree-sitter-scl#readme",
    "files": [
        "grammar.js",
        "src",
        "bindings",
        "queries"
    ]
}
